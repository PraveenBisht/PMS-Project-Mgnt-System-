CREATE PROCEDURE [dbo].[EmpTaskList]     
    @FromRow int,
	@userId varchar(100)
    --@PgSize int    
AS    
BEGIN    
declare @FromRow1 int;  
declare @TotalUser int=0;  
declare @PgSize int;  
declare @PgSize1 int; 
set @PgSize=10
set @TotalUser = (select count(TaskId) from [tbl_Task]where CreatedTo =@userId) 
set @FromRow = @TotalUser-@FromRow+1;  
  ;WITH RecordsRN AS  
  ( 
  select uu.TaskId,uu.SummaryName as TaskName, CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status  
  ,uu.Description,CONVERT(VARCHAR(11),FORMAT (uu.Createdon, 'yyyy MMM dd'))Createdon,j.ProjectName,usr.Name as Createdby,usr1.Name as CreatedTo,
  ROW_NUMBER() over(order by uu.TaskId) as Num  
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  where (uu.CreatedTo =@userId)
  ) 
  select TaskId,ProjectName,TaskName,isnull(RTRIM(Createdby),'')Createdby,isnull(RTRIM(CreatedTo),'')CreatedTo,Status,Createdon from RecordsRN WHERE Num between (@FromRow-@PgSize+1)AND @FromRow  order by Createdon
END






GO
/****** Object:  StoredProcedure [dbo].[EmpTaskList1]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[EmpTaskList1]     
    @FromRow int,
	@userId varchar(100)
    --@PgSize int    
AS    
BEGIN    
declare @FromRow1 int;  
declare @TotalUser int=0;  
declare @PgSize int;  
declare @PgSize1 int; 
set @PgSize=10
set @TotalUser = (select count(TaskId) from [tbl_Task]where CreatedTo =@userId) 
set @FromRow = @TotalUser-@FromRow+1;  
  ;WITH RecordsRN AS  
  ( 
  select uu.TaskId,uu.SummaryName as TaskName, 
  
  CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status ,
  CASE WHEN asst.Status =1 THEN 'Open' WHEN asst.Status =2 THEN 'Re-open' WHEN asst.Status =3 THEN 'Completed' END LastStatus ,uu.Description as Description,asst.Description as LastDescription,
  
  CONVERT(VARCHAR(11),FORMAT (getdate(), 'yyyy MMM dd'))Createdon,j.ProjectName,usr.Name as Createdby,usr1.Name as CreatedTo,
  ROW_NUMBER() over(order by uu.TaskId) as Num  
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  left join AssignTask asst on uu.TaskId=asst.TaskId where (uu.CreatedTo = @userId)
  ) 
  select TaskId,ProjectName,TaskName,isnull(RTRIM(Createdby),'')Createdby,isnull(RTRIM(CreatedTo),'')CreatedTo,Status,Createdon from RecordsRN WHERE Num between (@FromRow-@PgSize+1)AND @FromRow order by CreatedOn desc
END
select * from tbl_Task tt join AssignTask asst on tt.TaskId=asst.TaskId where tt.TaskId='2a53af45-3a88-4858-af29-90aab01ada07' order by asst.CreatedOn desc





GO
/****** Object:  StoredProcedure [dbo].[ManagerListForDDL]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[ManagerListForDDL]     

AS    
BEGIN    
select usr.Id,usr.Name from AspNetUsers usr join  AspNetUserRoles Rol on usr.Id=Rol.UserId left join AspNetRoles RR on Rol.RoleId=RR.Id
where RR.Name='Manager' 
END
GO
