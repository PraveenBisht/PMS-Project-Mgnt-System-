GO

CREATE PROCEDURE [dbo].[EmpListForDDL]     
	@EmpId varchar(100),
	@Role varchar(15)
AS    
BEGIN    
--Only Employee List
select usr.Id,usr.Name from AspNetUsers usr join  AspNetUserRoles Rol on usr.Id=Rol.UserId left join AspNetRoles RR on Rol.RoleId=RR.Id
where RR.Name=@Role and usr.Id<>@EmpId
--Only Employee List
--select usr.Id,usr.Name from AspNetUsers usr join  AspNetUserRoles Rol on usr.Id=Rol.UserId left join AspNetRoles RR on Rol.RoleId=RR.Id
--where RR.Name='User'

END
GO
