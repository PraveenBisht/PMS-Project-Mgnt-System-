CREATE PROCEDURE [dbo].[uspReAssignTask]
(
@TaskId varchar(70),
@FromassignId varchar(70),
@ToAssignId varchar(70), 
@Status int,
@Description varchar(800),
@CreatedOn varchar(20)
)
AS
BEGIN
insert into AssignTask(TaskId,FromassignId,ToAssignId,Status,Description,CreatedOn)
values(@TaskId,@FromassignId,@ToAssignId,@Status,@Description,Cast(@CreatedOn as datetime))
END
GO
