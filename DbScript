GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[EmpLeaveListWeb]     
	@userId varchar(100),
    @leave_type int,
	@RoleType varchar(10)
AS    
BEGIN 
	if(@RoleType = 'User' or @RoleType = 'AdminUser')
	Begin
		if(@leave_type>2)
			Begin
			select lt.pk_leave_taken_id
			,ll.leave_type,nu1.Name  fk_user_id
			,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
			,FORMAT (lt.LeaveDateTo, 'yyyy MMM dd')LeaveDateTo
			,isnull(lt.TotalLeave,1)TotalLeave
			,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
			from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
			lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
			where lt.fk_leave_type in (3,4,5,6) and lt.fk_user_id =@userId order by lt.LeaveDateFrom desc
			End
			else
			Begin
			select lt.pk_leave_taken_id
			,ll.leave_type,nu1.Name  fk_user_id
			,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
			,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateFrom, 'hh:mm tt') else '' end LeaveTimeFrom
			,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateTo, 'hh:mm tt') else '' end LeaveTimeTo
			,isnull(lt.TotalLeave,1)TotalLeave
			,case when (lt.is_afternoon = 0) then 'First Half'
			when (lt.is_afternoon = 1) then 'Second Half' else '' end as is_halfday
			,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
			 from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
			lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
			where lt.fk_leave_type=@leave_type and lt.fk_user_id=@userId order by lt.LeaveDateFrom desc
		End
	End
	if(@RoleType='Admin')
	Begin
		if(@leave_type>2)
			Begin
			select lt.pk_leave_taken_id
			,nu.Name UserName
			,ll.leave_type,nu1.Name  fk_user_id
			,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
			,FORMAT (lt.LeaveDateTo, 'yyyy MMM dd')LeaveDateTo
			,isnull(lt.TotalLeave,1)TotalLeave
			,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
			 from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
			lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
			where lt.fk_leave_type in (3,4,5,6) order by lt.LeaveDateFrom desc
			End
			else
			Begin
			select lt.pk_leave_taken_id
			,nu.Name UserName
			,ll.leave_type,nu1.Name  fk_user_id
			,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
			,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateFrom, 'hh:mm tt') else '' end LeaveTimeFrom
			,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateTo, 'hh:mm tt') else '' end LeaveTimeTo
			,isnull(lt.TotalLeave,1)TotalLeave
			,case when (lt.is_afternoon = 0) then 'First Half'
			when (lt.is_afternoon = 1) then 'Second Half' else '' end as is_halfday
			,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
			 from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
			lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
			where lt.fk_leave_type=@leave_type order by lt.LeaveDateFrom desc
		End
	End
	if(@RoleType='Manager')
	Begin
		if(@leave_type>2)
			Begin
			select lt.pk_leave_taken_id
			,nu.Name UserName
			,ll.leave_type,nu1.Name  fk_user_id
			,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
			,FORMAT (lt.LeaveDateTo, 'yyyy MMM dd')LeaveDateTo
			,isnull(lt.TotalLeave,1)TotalLeave
			,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
			 from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
			lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
			where (lt.fk_leave_type in (3,4,5,6)) and (lt.leave_status !=2) and (lt.fk_approved_user_id=@userId) order by lt.LeaveDateFrom desc
			End
			else
			Begin
			select lt.pk_leave_taken_id
			,nu.Name UserName
			,ll.leave_type,nu1.Name  fk_user_id
			,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
			,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateFrom, 'hh:mm tt') else '' end LeaveTimeFrom
			,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateTo, 'hh:mm tt') else '' end LeaveTimeTo
			,isnull(lt.TotalLeave,1)TotalLeave
			,case when (lt.is_afternoon = 0) then 'First Half'
			when (lt.is_afternoon = 1) then 'Second Half' else '' end as is_halfday
			,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
			 from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
			lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
			where lt.fk_leave_type=@leave_type and (lt.leave_status !=2)and (lt.fk_approved_user_id=@userId) order by lt.LeaveDateFrom desc
		End
	End
END
GO
