CREATE PROCEDURE [dbo].[EmpReOpenTaskList_ByTwoDate]     
    @FromRow int,
	@userId varchar(100),
	@Fromdate varchar(100),
	@Todate varchar(100)
    --@PgSize int    
AS    
BEGIN    
declare @FromRow1 int;  
declare @TotalUser int=0;  
declare @PgSize int;  
declare @PgSize1 int; 
set @PgSize=100
set @TotalUser = (select count(TaskId) from [tbl_Task]where (CreatedTo =@userId) and Createdon BETWEEN @Fromdate AND @Todate  and Status=2) 
set @FromRow = @TotalUser-@FromRow+1;  
  ;WITH RecordsRN AS  
  ( 
  select uu.TaskId,uu.SummaryName as TaskName, CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status  
  ,uu.Description,CONVERT(VARCHAR(11),FORMAT (uu.Createdon, 'yyyy MMM dd'))Createdon,j.ProjectName,usr.Name as Createdby,usr1.Name as CreatedTo,
  ROW_NUMBER() over(order by uu.TaskId) as Num  
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  where (uu.CreatedTo =@userId) and (uu.Createdon BETWEEN @Fromdate AND @Todate)  and uu.Status=2
  ) 
  select TaskId,ProjectName,TaskName,isnull(RTRIM(Createdby),'')Createdby,isnull(RTRIM(CreatedTo),'')CreatedTo,Status,Createdon from RecordsRN WHERE Num between (@FromRow-@PgSize+1)AND @FromRow  order by Createdon
END
