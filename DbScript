CREATE PROCEDURE [dbo].[EmpLeaveList_ByTwoDate]     
    @FromRow int,
	@userId varchar(100),
    @leave_status int,
	@Fromdate varchar(100),
	@Todate varchar(100)
AS    
BEGIN       
declare @FromRow1 int;  
declare @TotalUser int=0;  
declare @PgSize int;  
declare @PgSize1 int; 
set @PgSize=10
set @TotalUser = (select count(pk_leave_taken_id) from Leave_Management_Leave_Taken where leave_status=@leave_status) 
set @FromRow = @TotalUser-@FromRow+1;  
  ;WITH RecordsRN AS  
  ( 
  select lt.pk_leave_taken_id
,nu.Name  fk_user_id,
lt.leave_days daydate,
lt.fk_user_id id
,lt.fk_leave_type
,ll.leave_type
,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
,FORMAT (lt.LeaveDateTo, 'yyyy MMM dd')LeaveDateTo
,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateFrom, 'hh:mm tt') else '' end LeaveTimeFrom
,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateTo, 'hh:mm tt') else '' end LeaveTimeTo
,isnull(lt.TotalLeave,1)TotalLeave
,case when (lt.is_afternoon = 0) then 'First Half'
when (lt.is_afternoon = 1) then 'Second Half' else '' end as is_halfday
,lt.leave_status
,nu1.Name
,lt.Remark,
ROW_NUMBER() over(order by lt.pk_leave_taken_id) as Num  from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
where lt.leave_status=@leave_status and lt.fk_user_id=@userId and (lt.leave_days between @Fromdate and @todate)  
)
  select * from RecordsRN 
END
GO
/****** Object:  StoredProcedure [dbo].[EmpLeaveListWeb]    Script Date: 11-11-2019 18:17:29 ******/
