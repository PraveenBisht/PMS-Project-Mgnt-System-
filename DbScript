CREATE PROCEDURE [dbo].[DashboardDetails]     
	@EmpId varchar(100),
	@Role varchar(100)   
AS    
BEGIN    
select count(id)TotalTask, sum(case when Status = 1 then 1 else 0 end) OpenTask,sum(case when Status = 2 then 1 else 0 end) ReOpenTask,
sum(case when Status = 3 then 1 else 0 end) CompletedTask from tbl_Task where CreatedTo = 'a1a9e1b0-2567-4e85-af74-821d57fbc7ea'
END
GO
/****** Object:  StoredProcedure [dbo].[EmpCompletedTaskDetail]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[EmpCompletedTaskDetail]     
	@userId varchar(100),
	@TaskId varchar(100) 
AS    
BEGIN    
  select uu.TaskId,uu.SummaryName as TaskName, CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status  
  ,uu.Description,CONVERT(VARCHAR(11),FORMAT (uu.Createdon, 'yyyy MMM dd'))Createdon,j.ProjectName,isnull(RTRIM(usr.Name),'')Createdby,isnull(RTRIM(usr1.Name),'')CreatedTo 
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  where (uu.CreatedTo =@userId) and uu.Status=3 and TaskId=@TaskId
  select imageid, isnull(imageName,'')imageName from Filedetails where TaskId=@TaskId
 END
GO
/****** Object:  StoredProcedure [dbo].[EmpCompletedTaskList]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[EmpCompletedTaskList]     
    @FromRow int,
	@userId varchar(100)
    --@PgSize int    
AS    
BEGIN    
declare @FromRow1 int;  
declare @TotalUser int=0;  
declare @PgSize int;  
declare @PgSize1 int; 
set @PgSize=100
set @TotalUser = (select count(TaskId) from [tbl_Task]where (CreatedTo =@userId) and (Createdon= CONVERT(VARCHAR(10), getdate(), 111))  and Status=3) 
set @FromRow = @TotalUser-@FromRow+1;  
  ;WITH RecordsRN AS  
  ( 
  select uu.TaskId,uu.SummaryName as TaskName, CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status  
  ,uu.Description,CONVERT(VARCHAR(11),FORMAT (uu.Createdon, 'yyyy MMM dd'))Createdon,j.ProjectName,usr.Name as Createdby,usr1.Name as CreatedTo,
  ROW_NUMBER() over(order by uu.TaskId) as Num  
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  where (uu.CreatedTo =@userId) and uu.Createdon=CONVERT(VARCHAR(10), getdate(), 111) and uu.Status=3
  ) 
  select TaskId,ProjectName,TaskName,isnull(RTRIM(Createdby),'')Createdby,isnull(RTRIM(CreatedTo),'')CreatedTo,Status,Createdon from RecordsRN WHERE Num between (@FromRow-@PgSize+1)AND @FromRow  order by Createdon
END
GO
/****** Object:  StoredProcedure [dbo].[EmpCompletedTaskList_ByTwoDate]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[EmpCompletedTaskList_ByTwoDate]     
    @FromRow int,
	@userId varchar(100),
    @Fromdate varchar(100),
	@Todate varchar(100)
    --@PgSize int    
AS    
BEGIN    
declare @FromRow1 int;  
declare @TotalUser int=0;  
declare @PgSize int;  
declare @PgSize1 int; 
set @PgSize=100
set @TotalUser = (select count(TaskId) from [tbl_Task]where (CreatedTo =@userId) and Createdon BETWEEN @Fromdate AND @Todate and Status=3) 
set @FromRow = @TotalUser-@FromRow+1;  
  ;WITH RecordsRN AS  
  ( 
  select uu.TaskId,uu.SummaryName as TaskName, CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status  
  ,uu.Description,CONVERT(VARCHAR(11),FORMAT (uu.Createdon, 'yyyy MMM dd'))Createdon,j.ProjectName,usr.Name as Createdby,usr1.Name as CreatedTo,
  ROW_NUMBER() over(order by uu.TaskId) as Num  
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  where (uu.CreatedTo =@userId) and (uu.Createdon BETWEEN @Fromdate AND @Todate) and uu.Status=3
  ) 
  select TaskId,ProjectName,TaskName,isnull(RTRIM(Createdby),'')Createdby,isnull(RTRIM(CreatedTo),'')CreatedTo,Status,Createdon from RecordsRN WHERE Num between (@FromRow-@PgSize+1)AND @FromRow  order by Createdon
END
GO
/****** Object:  StoredProcedure [dbo].[EmpLeaveApproveReject]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[EmpLeaveApproveReject] 
	@LeaveId int,
	@leave_status int
AS    
BEGIN 
SET NOCOUNT ON;
 if(@LeaveId in(select pk_leave_taken_id from Leave_Management_Leave_Taken))
	 Begin
		update Leave_Management_Leave_Taken set leave_status = @leave_status where pk_leave_taken_id =@LeaveId 
	 End
 else 
	 Begin
		RAISERROR('Fail to Upadte the Record. ',16,1);
	 End

END
GO
/****** Object:  StoredProcedure [dbo].[EmpLeaveDetailByIdWeb]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE PROCEDURE [dbo].[EmpLeaveDetailByIdWeb] 
	@LeaveId int
AS    
BEGIN 
	select lt.pk_leave_taken_id,lt.fk_user_id,nu.Name UserName
	,ll.leave_type,nu1.Name  ManagerName
	,FORMAT (lt.LeaveDateFrom, 'yyyy MMM dd')LeaveDateFrom
	,FORMAT (lt.LeaveDateTo, 'yyyy MMM dd')LeaveDateTo
	,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateFrom, 'hh:mm tt') else '' end LeaveTimeFrom
	,case when (lt.fk_leave_type =1) then FORMAT (lt.LeaveDateTo, 'hh:mm tt') else '' end LeaveTimeTo
	,isnull(lt.TotalLeave,0)TotalLeave
	,case when (lt.is_afternoon = 0) then 'First Half'
	when (lt.is_afternoon = 1) then 'Second Half' else '' end as is_halfday
	,case when(lt.leave_status = 0) then 'Pending' when(lt.leave_status = 1) then 'Approved' when(lt.leave_status = 2) then 'Rejected' end as leave_status1
	,lt.Remark
		from Leave_Management_Leave_Taken lt left join Leave_Management_Leave_Type ll on 
	lt.fk_leave_type=ll.pk_leave_type_id left join AspNetUsers nu on nu.Id=lt.fk_user_id left join AspNetUsers nu1 on nu1.Id=lt.fk_approved_user_id  
	where lt.pk_leave_taken_id =@LeaveId 
END


GO
