CREATE PROCEDURE [dbo].[EmpReOpenTaskDetail]     
	@userId varchar(100),	
	@TaskId varchar(100)
    --@PgSize int    
AS    
BEGIN    
  select uu.TaskId,uu.SummaryName as TaskName, CASE WHEN uu.Status =1 THEN 'Open' WHEN uu.Status =2 THEN 'Re-open' WHEN uu.Status =3 THEN 'Completed' END Status  
  ,uu.Description,CONVERT(VARCHAR(11),FORMAT (uu.Createdon, 'yyyy MMM dd'))Createdon,j.ProjectName,isnull(RTRIM(usr.Name),'')Createdby,isnull(RTRIM(usr1.Name),'')CreatedTo 
  from [tbl_Task] uu join ProjectMaster j on j.ProjectID=uu.projectid left join AspNetUsers usr on usr.Id=uu.Createdby left join AspNetUsers usr1 on usr1.Id=uu.CreatedTo 
  where (uu.CreatedTo =@userId) and TaskId=@TaskId and uu.Status=2
   select imageid, isnull(imageName,'')imageName from Filedetails where TaskId=@TaskId
END
GO
