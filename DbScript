/****** Object:  StoredProcedure [dbo].[Update_Chat_Message_IsRead_Status]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[Update_Chat_Message_IsRead_Status]
        @FromEmailId VARCHAR(50),
	    @ToEmailId VARCHAR(50)
AS
BEGIN
	SET NOCOUNT ON;
	UPDATE ChatPrivateMessageDetails SET IsRead = 1
    WHERE MasterEmailID=@ToEmailId and ChatToEmailID=@FromEmailId and IsRead=0	 
END
GO
/****** Object:  StoredProcedure [dbo].[uspAssignTask]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[uspAssignTask]
(
@TaskId varchar(70),
@FromassignId varchar(70),
@ToAssignId varchar(70), 
@Status int,
@Description varchar(800),
@CreatedOn varchar(20)
)
AS
BEGIN
SET NOCOUNT OFF
insert into AssignTask(TaskId,FromassignId,ToAssignId,Status,Description,CreatedOn)
values(@TaskId,@FromassignId,@ToAssignId,@Status,@Description,Cast(@CreatedOn as datetime))
END
GO
/****** Object:  StoredProcedure [dbo].[uspComment]    Script Date: 11-11-2019 18:17:29 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[uspComment]
(
@TaskId varchar(100),
@ToAssignId varchar(100), 
@Status int,
@Description varchar(800),
@CreatedOn varchar(20)
)
AS
BEGIN
declare @FromassignId varchar(100)
Select @FromassignId = (select top 1 (FromassignId) from AssignTask where ToAssignId=@ToAssignId and TaskId=@TaskId)
if( @FromassignId is null) 
BEGIN Select @FromassignId = (select top 1 (Createdby) from tbl_Task where CreatedTo=@ToAssignId and TaskId=@TaskId) END

insert into AssignTask(TaskId,FromassignId,ToAssignId,Status,Description,CreatedOn)
values(@TaskId,@FromassignId,@ToAssignId,@Status,@Description,Cast(@CreatedOn as datetime))
END

GO
/****** Object:  StoredProcedure [dbo].[uspReAssignTask]    Script Date: 11-11-2019 18:17:29 ******/
